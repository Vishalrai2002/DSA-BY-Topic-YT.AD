#include<iostream>
using namespace std;
int main()
{
    // Lower Case to Upper Case
    string str="vishal";
    for (int i=0;i<str.size();i++)
    {
        if (str[i]>-'a' && str[i]<='z'){
            str[i]-=32;
        }
    }
    cout<<str<<endl;
    
    // Upper Case to Lower Case
    string str1="VISHAL";
    for (int i=0;i<str1.size();i++)
    {
        if (str1[i]>='A'&&str1[i]<='Z'){
            str1[i]+=32;
        }
    }
    cout<<str1<<endl;
    return 0;
}
